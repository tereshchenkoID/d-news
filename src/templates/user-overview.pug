---
title: D-news | Overview
lang: en
---
extends layouts/_layout

block content
  +header(true, 0)#header

  main.main
    section.section
      .container-fluid
        .container
          .widget
            .widget__header
              +widget-title('My account', 'logout').widget-title--alt

          .panel.u-mb-30
            h4 Hello,
            p.u-mb-30 Find an overview of your stored information here and edit your data and subscriptions conveniently from home.
            .row.u-mb-16
              .col.col-6
                h6.u-mb-0 User Data
              .col.col-6
                +button('Edit')(href="./user-edit.html").button--primary.button--sm.button--inline
            .row
              .col.col-md-6.u-pb-10
                strong Name
              .col.col-md-6.u-pb-10
                p N / A.
              .col.col-md-6.u-pb-10
                strong Email
              .col.col-md-6.u-pb-10
                p tereschenko23041991@gmail.com
              .col.col-md-6.u-pb-10
                strong Password
              .col.col-md-6.u-pb-10
                p *****
              .col.col-md-6
                strong Customer number
              .col.col-md-6
                p N / A.

          .panel.u-mb-30
            h4 Data deletion according to DSGVO
            +button('For data deletions')(href="./user-delete.html").button--animate.button--sm.button--inline

          .row
            .col.col-md-4.col-verticall-offset
              .address.u-text-center
                .address__icon
                  i.fa.fa-map-signs
                h3.address__title.u-text-transform-capitalize.u-mb-0 Address
                p.address__text Suite 456 New York, NY 123456
            .col.col-md-4.col-verticall-offset
              .address.u-text-center
                .address__icon
                  i.fa.fa-phone
                h3.address__title.u-text-transform-capitalize.u-mb-0 Mobile number
                p.address__text +012 345 678 910
            .col.col-md-4.col-verticall-offset
              .address.u-text-center
                .address__icon
                  i.fa.fa-envelope
                h3.address__title.u-text-transform-capitalize.u-mb-0 Email address
                p.address__text info@gmail.com
