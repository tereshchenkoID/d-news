mixin social
  ul.social&attributes(attributes)
    li.social__item
      a(href="#" aria-label="Facebook").social__link
        i.fab.fa-facebook-f.social__icon
    li.social__item
      a(href="#" aria-label="Twitter").social__link
        i.fab.fa-twitter.social__icon
    li.social__item
      a(href="#" aria-label="Google").social__link
        i.fab.fa-google.social__icon
    li.social__item
      a(href="#" aria-label="Instagram").social__link
        i.fab.fa-instagram.social__icon
    li.social__item
      a(href="#" aria-label="Youtube").social__link
        i.fab.fa-youtube.social__icon
